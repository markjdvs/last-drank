<div class="container">
  <% for(const type in messages) { %>
    <div class="<%= type %>">
      <p><%= messages[type] %></p>
    </div>
  <% } %>
  <div class="row dropFromNav">
    <h4><%= user.username %></h4>
  </div>
  <% twists.forEach((twist) => { %>
    <div class="row dropFromNav">
      <div class="col-lg-6">
        <img class="img-fluid" src="<%= twist.imageSRC %>" alt="<%= twist.name %>">
      </div>
      <div class="col-lg-6">
        <h5><%= twist.name %></h5>
        <p><%= twist.recipe %></p>
      </div>
    </div>
  <% }) %>
</div>
